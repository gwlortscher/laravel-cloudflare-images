{
  "name": "koraycicekciogullari/laravel-cloudflare-images",
  "description": "Cloudflare Images for Flysystem",
  "license": "MIT",
  "version": "1.0.0",
  "keywords": [
    "laravel",
    "cloudflare",
    "images"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Koray Çiçekçioğulları",
      "email": "k.cicekciogullari@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0",
    "laravel/framework": "^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.4|^9.0",
    "league/flysystem-adapter-test-utilities": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Koraycicekciogullari\\LaravelCloudflareImages\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Koraycicekciogullari\\LaravelCloudflareImages\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Koraycicekciogullari\\LaravelCloudflareImages\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
